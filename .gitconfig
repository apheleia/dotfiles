[include]
	path = ~/.gitconfigemail
[user]
	name = Martin Schenck
	email = martinschenck@fastmail.com
[core]
	editor = kak
[alias]
  o = checkout
  c = commit
  a = add -A
  s = status
  b = branch
  bd = branch -d
  l = log --pretty=short --max-count=10
  d = diff
  ds = diff --staged
  r = rebase
  p = pull -p
  h = push
[pull]
	rebase = false
